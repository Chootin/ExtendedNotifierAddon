{
    "manifest_version": 2,
    "name": "Facebook Extra Notifier",
    "version": "1.0",
    "page_action": {
        "default_icon": "graphics/facebook_icon_32.png",
        "default_title": "Facebook Extra Notifier"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.facebook.com/*"
            ],
            "js": ["javascript/facebook_extra_notifier.js"]
        }
    ],
    "background": {
        "scripts": []
    },
    "permissions": [
    ],
    "icons": {
        "16": "graphics/facebook_icon_16.png",
        "32": "graphics/facebook_icon_32.png",
        "48": "graphics/facebook_icon_48.png",
        "64": "graphics/facebook_icon_64.png",
        "128": "graphics/facebook_icon_128.png"
    }
}
